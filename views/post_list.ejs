<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>

<body>
<h1>Posts</h1>

<nav>
    <%if (previous) { %>
        <a href="<%= previous %>">Previous</a>
    <% } %>
    <%if (next) { %>
        <a href="<%= next %>">Next </a>
    <% }%>
</nav>
<div class = "padding">
<ol>
    <% postList.results.forEach(function(post) {%>
        <li class="post_list_text">
            <a href="/posts/<%= post.id %>" style="text-decoration:none;"><%= post.message %></a>
            <a href="/posts/<%= post.id %>"><img src="<%=post.imageUrl%>" class="post_list_image" alt="user image" width=300 height=300/></a>
            Posted by 
            <a href="/users/<%=post.postedBy.id %>" style="text-decoration:none;"> <%= post.postedBy.name%> </a>
            <%= post.postedBy.email %>
            <div>
                <img src="data:image/jpeg;base64," width=40/>
                <%= post.likedBy.length%>
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRsbJpogpSXGRUW_b_O1Uq8jGgwj5KN5mK8WA&usqp=CAU" width=40/>
                <%= post.dislikedBy.length%>
            </div>
        </li>
    <% }) %>
</ol>
</div>
</body>

<%- include('_footer') -%>
</html>


