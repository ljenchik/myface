<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>

<head>
    <style>
        body {background-color: lightblue;}
        h1 {color: rgb(29, 19, 162); margin-left: 20px;}
    </style>
</head>


<body>
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <img src="<%=user.profileImageUrl%>" alt="user image" width=20 height=20/>
            
            <a href="users/<%=user.id %>"> <%= user.name %></a>
        </li>
    <% }); %>
</ol>
<%if (next) { %>
    <a href="<%= next %>"> Next </a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>"> Previous </a>
<% } %>
</body>
<%- include('_footer') -%>
</html>

